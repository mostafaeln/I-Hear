{
  "expo": {
    "name": "I-Hear",
    "slug": "I-Hear",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./Images/ihear-2.png",
      "resizeMode": "contain",
      "left":"40",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./Images/Ihear.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          
          "icon": "./local/Images/sound.png",
          "color": "#ffffff",
          "sounds": [
            "./local/notification.wav"
          ]
        }
      ]
    ]
  }
}